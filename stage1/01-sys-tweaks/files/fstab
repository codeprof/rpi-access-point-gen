proc            /proc           proc    defaults          0       0
BOOTDEV  /boot           vfat    ro,defaults          0       2
ROOTDEV  /               ext4    ro,defaults,nodiratime,noatime  0       1
tmpfs           /var/log        tmpfs   nodev,nosuid,size=100M          0 0
tmpfs           /var/tmp        tmpfs   nodev,nosuid,size=100M          0 0
tmpfs           /tmp            tmpfs   nodev,nosuid,size=250M          0 0
